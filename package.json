{
  "name": "expo-template",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "start-dev": "NODE_ENV=development expo start",
    "start-staging": "NODE_ENV=staging expo start",
    "start-prod": "NODE_ENV=production expo start",
    "eas:update": "eas update --branch main --message 'CI/CD Update'",
    "build-android-dev": "NODE_ENV=development eas build --platform android --profile development",
    "build-android-staging": "NODE_ENV=staging eas build --platform android --profile staging",
    "build-android-prod": "NODE_ENV=production eas build --platform android --profile production",
    "build-android-dev-local": "NODE_ENV=development eas build --platform android --profile development --local",
    "build-android-staging-local": "NODE_ENV=staging eas build --platform android --profile staging --local",
    "build-ios-dev": "NODE_ENV=development eas build --platform ios --profile development",
    "build-ios-staging": "NODE_ENV=staging eas build --platform ios --profile staging",
    "build-ios-prod": "NODE_ENV=production eas build --platform ios --profile production",
    "build-local-credential-ios": "NODE_ENV=development eas build --platform android --profile development-local",
    "submit-android-prod": "eas submit --platform android --profile production --latest",
    "submit-ios-prod": "eas submit --platform ios --profile production --latest",
    "build-and-submit-android-prod": "npm run build-android-prod && eas submit --platform android --profile production --latest",
    "build-and-submit-ios-prod": "npm run build-ios-prod && eas submit --platform ios --profile production --latest",
    "push-dev": "NODE_ENV=development eas update --channel development",
    "push-staging": "NODE_ENV=staging eas update --channel staging",
    "push-prod": "NODE_ENV=production eas update --channel production",
    "android": "expo run:android",
    "generate-keystore": "bash scripts/create-keystore.sh",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "expo lint"
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "^3.2.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/elements": "^2.1.6",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "1.7.6",
    "@tanstack/react-query": "^5.61.5",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "expo": "^53.0.12",
    "expo-asset": "~11.1.5",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.8",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.1",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-location": "~18.1.5",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-updates": "~0.28.15",
    "expo-web-browser": "~14.2.0",
    "i18next": "^24.0.5",
    "lodash": "^4.17.21",
    "nativewind": "^4.1.23",
    "numeral": "^2.0.6",
    "onesignal-expo-plugin": "^2.0.3",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.1.3",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-modalize": "^2.1.1",
    "react-native-onesignal": "^5.2.8",
    "react-native-reanimated": "~3.17.4",
    "react-native-root-siblings": "^5.0.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "yup": "^1.5.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.0",
    "@types/numeral": "^2.0.5",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "metro": "^0.82.4",
    "postcss": "^8.4.49",
    "react-test-renderer": "^19.0.0",
    "tailwindcss": "^3.4.16",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "collectCoverage": true,
    "automock": false,
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/expo-env.d.ts",
      "!**/.expo/**"
    ]
  }
}
