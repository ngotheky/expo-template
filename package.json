  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.0",
    "@types/numeral": "^2.0.5",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "metro": "^0.81.0",
    "postcss": "^8.4.49",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.3.3"
  }
}
