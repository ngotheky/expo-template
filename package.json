{
  "name": "amela-expo-template",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "start-dev": "NODE_ENV=development expo start",
    "start-preview": "NODE_ENV=preview expo start",
    "start-prod": "NODE_ENV=production expo start",
    "build-android-dev": "NODE_ENV=development eas build --platform android --profile development",
    "build-android-staging": "NODE_ENV=staging eas build --platform android --profile preview",
    "build-android-prod": "NODE_ENV=production eas build --platform android --profile production",
    "build-android-dev-local": "NODE_ENV=development eas build --platform android --profile development --local",
    "build-android-staging-local": "NODE_ENV=staging eas build --platform android --profile preview --local",
    "build-ios-dev": "NODE_ENV=development eas build --platform ios --profile development",
    "build-ios-staging": "NODE_ENV=staging eas build --platform ios --profile preview",
    "build-ios-prod": "NODE_ENV=production eas build --platform ios --profile production",
    "build-local-credential-ios": "NODE_ENV=development eas build --platform android --profile development-local",
    "submit-android-prod": "eas submit --platform android --profile production --latest",
    "submit-ios-prod": "eas submit --platform ios --profile production --latest",
    "build-and-submit-android-prod": "npm run build-android-prod && eas submit --platform android --profile production --latest",
    "build-and-submit-ios-prod": "npm run build-ios-prod && eas submit --platform ios --profile production --latest",
    "push-dev": "NODE_ENV=development eas update --channel development",
    "push-staging": "NODE_ENV=staging eas update --channel preview",
    "push-prod": "NODE_ENV=production eas update --channel production",
    "android": "expo run:android",
    "generate-keystore-prod": "cp .env.production .env && eas credentials",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@alessiocancian/react-native-actionsheet": "^3.2.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/elements": "^2.1.6",
    "@react-navigation/native": "^7.0.0",
    "@shopify/flash-list": "1.7.1",
    "@tanstack/react-query": "^5.61.5",
    "axios": "^1.7.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "expo": "~52.0.11",
    "expo-asset": "~11.0.1",
    "expo-blur": "~14.0.1",
    "expo-camera": "~16.0.9",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.6",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-image-picker": "~16.0.3",
    "expo-linking": "~7.0.3",
    "expo-localization": "~16.0.0",
    "expo-location": "~18.0.4",
    "expo-media-library": "~17.0.3",
    "expo-notifications": "~0.29.11",
    "expo-router": "^3.1.0",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.5",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "i18next": "^24.0.5",
    "lodash": "^4.17.21",
    "nativewind": "^4.1.23",
    "numeral": "^2.0.6",
    "onesignal-expo-plugin": "^2.0.3",
    "patch-package": "^8.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.1.3",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-maps": "1.18.0",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-modalize": "^2.1.1",
    "react-native-onesignal": "^5.2.8",
    "react-native-reanimated": "~3.16.1",
    "react-native-root-siblings": "^5.0.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-web": "~0.19.13",
